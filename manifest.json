{
    "name": "Save My Tabs",
    "version": "1.0.1",
    "description": "Save list of open tabs to Dropbox",
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "options_page": "options.html",
    "permissions": [
        "identity",
        "storage",
        "tabs",
        "https://www.dropbox.com/oauth2/*"
    ],
    "key": "",
    "manifest_version": 2
}
